{
  "name": "@fleur/froute",
  "version": "0.1.0",
  "description": "Type safe and flexible router for React",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "repository": "git@github.com:fleur-js/froute.git",
  "author": "Hanakla <ragg.devpr@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "clean-dist": "rm -rf ./dist",
    "build:declarations": "tsc --emitDeclarationOnly --declaration --outDir ./dist",
    "build:esm": "bili",
    "build": "yarn build:declarations && yarn build:esm",
    "prepublishOnly": "yarn clean-dist && yarn build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "peerDependencies": {
    "react": ">= 16.8.x"
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.2",
    "@testing-library/react-hooks": "^3.7.0",
    "@types/history": "^4.7.7",
    "@types/jest": "^26.0.10",
    "@types/path-to-regexp": "^1.7.0",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "bili": "^5.0.5",
    "cross-env": "^7.0.2",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "lint-staged": "^10.2.11",
    "prettier": "^2.1.1",
    "react": "^16.13.1",
    "react-dom": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "history": "^5.0.0",
    "path-to-regexp": "^6.1.0",
    "querystring": "^0.2.0",
    "url": "^0.11.0"
  }
}
